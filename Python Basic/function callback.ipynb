     "output_type": "stream",
     "text": [
      " square of x and then cube is  15625\n"
     ]
    }
   ],
   "source": [
    "def sqr(x, callback):\n",
    "    y = x**2\n",
    "    callback(y)\n",
    "\n",
    "def cube(y):\n",
    "    c = y**3\n",
    "    print(\" square of x and then cube is \", c)\n",
    "sqr(5, cube)"
   ]
